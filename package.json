    "@babel/preset-typescript": "7.22.11",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "eslint": "8.48.0",
    "html-webpack-plugin": "5.5.3",
    "node-sass": "9.0.0",
    "prettier": "3.0.3",
    "sass": "1.66.1",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
